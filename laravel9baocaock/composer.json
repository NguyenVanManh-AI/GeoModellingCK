{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1.2", 
        "aws/aws-sdk-php-laravel": "^3.8",
        "brian2694/laravel-toastr": "5.57",
        "cloudinary-labs/cloudinary-laravel": "^2.0",
        "darkaonline/l5-swagger": "^8.6",
        "fruitcake/laravel-cors": "^2.0.5",
        "guzzlehttp/guzzle": "^7.8",
        "laravel/framework": "^9.0",
        "laravel/pint": "^1.13",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "2.7",
        "league/flysystem-aws-s3-v3": "^3.24",
        "predis/predis": "^2.2",
        "socialiteproviders/github": "4.1",
        "socialiteproviders/gitlab": "4.1",
        "socialiteproviders/google": "4.1",
        "socialiteproviders/twitter": "4.1",
        "tomfordrumm/jwt-auth": "dev-develop"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "brainmaestro/composer-git-hooks": "*",
        "fakerphp/faker": "^1.9.1",
        "larastan/larastan": "2.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "analyze-min":  "vendor/bin/phpstan analyze",
        "analyze":  "vendor/bin/phpstan analyze --memory-limit=2G ",
        "analyze-debug":  "vendor/bin/phpstan analyze --memory-limit=2G --debug",
        "baseline":  "vendor/bin/phpstan analyse --generate-baseline --memory-limit=2G"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
